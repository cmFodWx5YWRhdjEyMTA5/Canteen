                                android:layout_width="50dp"
                                android:shape="ring"
                                app:cardCornerRadius="25dp"
                                android:layout_height="50dp">
                                <ImageView
                                    android:layout_gravity="center"
                                    android:layout_width="50dp"
                                    android:layout_height="50dp"
                                    android:src="@drawable/ic_coffee"
                                    />
